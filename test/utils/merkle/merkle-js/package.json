{
  "name": "v2-contracts-merkle-pregeneration",
  "version": "1.0.0",
  "description": "Merkle tree pre-generation for optimized test execution",
  "main": "deterministic-merkle-pregeneration.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "merkle:pre-gen": "node deterministic-merkle-pregeneration.js",
    "merkle:force": "node deterministic-merkle-pregeneration.js --force",
    "merkle:verbose": "node deterministic-merkle-pregeneration.js --verbose",
    "merkle:status": "node deterministic-merkle-pregeneration.js --status",
    "build-cache": "node deterministic-merkle-pregeneration.js",
    "list-hooks": "node efficient-proof-lookup.js list",
    "test:with-merkle": "npm run merkle:pre-gen && cd ../../../.. && forge test",
    "test:performance": "make -f Makefile.merkle-pre-gen test-performance-comparison"
  },
  "keywords": [
    "ethereum",
    "solidity",
    "merkle-tree",
    "foundry",
    "testing",
    "optimization"
  ],
  "author": "Superform",
  "license": "UNLICENSED",
  "dependencies": {
    "@openzeppelin/merkle-tree": "^1.0.7",
    "ethers": "^5.8.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  }
}